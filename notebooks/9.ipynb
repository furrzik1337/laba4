    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "# Создаем Series из чисел\n",
    "data = [2, 4, 6, 8, 10]\n",
    "series = pd.Series(data)\n",
    "\n",
    "# Применяем функцию вычисления квадратного корня с помощью .apply(np.sqrt)\n",
    "sqrt_series = series.apply(np.sqrt)\n",
    "\n",
    "# Выводим результат\n",
    "print(sqrt_series)\n"
   ]
